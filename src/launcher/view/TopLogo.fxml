<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import java.net.URL?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="topLogo" xmlns="http://javafx.com/javafx/8"
            fx:controller="launcher.controller.TopLogoController">
    <stylesheets>
        <URL value="@/launcher/view/styleSheets/TopLogo.css"/>
    </stylesheets>
    <center>
        <Label id="launcherName" text="GameMaster" BorderPane.alignment="CENTER"/>
    </center>
    <right>
        <Label id="companyName" text="Birdstufz" BorderPane.alignment="CENTER_RIGHT"/>
    </right>
    <left>
        <ImageView fitHeight="160.0" fitWidth="275.0" pickOnBounds="true" preserveRatio="true"
                   BorderPane.alignment="CENTER_LEFT">
            <image>
                <Image url="@../resources/Logo.png"/>
            </image>
        </ImageView>
    </left>
</BorderPane>
